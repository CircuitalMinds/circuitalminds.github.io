---
layout: null
---
[
  {% for post in site.posts %}
    {
      "title"    : "{{ post.title | escape }}",
      "category" : "{{ post.category }}",
      "tags"     : "{{ post.tags | array_to_sentence_string }}",
      "url"      : "{{ post.url }}",
      "date"     : "{{ post.date }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
  ,
  {% for video in site.data.videos %}
    { 
      "description" : "{{ video[1].description | escape }}",
      "tag"         : "{{ video[1].tag | escape }}",
      "name"        : "{{ video[1].name | escape }}",
      "title"       : "{{ video[1].title | escape }}",
      "url"         : "{{ video[1].url }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}    
]